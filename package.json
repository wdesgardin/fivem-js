{
  "name": "@wdesgardin/fivem-js",
  "version": "1.6.8",
  "description": "Javascript/Typescript wrapper for the FiveM natives",
  "sideEffects": [
    "./lib/models/*.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "check": "prettier --check \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags",
    "typedoc": "typedoc",
    "docs": "yarn typedoc -- --options typedoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wdesgardin/fivem-js.git"
  },
  "keywords": [
    "fivem",
    "wrapper",
    "javascript",
    "typescript",
    "citizenfx"
  ],
  "files": [
    "lib/**/*"
  ],
  "author": "d0p3t,wdesgardin",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "lint-staged": "^11.2.3",
    "prettier": "^2.3.0",
    "typedoc": "^0.22.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@citizenfx/client": "^2.0.4748-1"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix --ext ts",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  }
}
